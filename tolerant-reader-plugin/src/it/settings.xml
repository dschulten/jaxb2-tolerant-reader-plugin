<?xml version="1.0"?>
<settings>
  <proxies>
    <proxy>
      <id>dzbw-proxy</id>
      <active>true</active>
      <protocol>http</protocol>
      <host>squid.dzbw.de</host>
      <port>3128</port>
      <nonProxyHosts>127.0.0.1|localhost|*.dzbw.de</nonProxyHosts>
    </proxy>
  </proxies>
  <profiles>
    <profile>
      <id>it-repo</id>
      <activation>
        <activeByDefault>true</activeByDefault>
      </activation>
      <repositories>
        <repository>
          <id>local.central</id>
          <url>@localRepositoryUrl@</url>
          <releases>
            <enabled>true</enabled>
          </releases>
          <snapshots>
            <enabled>true</enabled>
          </snapshots>
        </repository>
      </repositories>
      <pluginRepositories>
        <pluginRepository>
          <id>local.central</id>
          <url>@localRepositoryUrl@</url>
          <releases>
            <enabled>true</enabled>
          </releases>
          <snapshots>
            <enabled>true</enabled>
          </snapshots>
        </pluginRepository>
      </pluginRepositories>
    </profile>
  </profiles>
</settings>